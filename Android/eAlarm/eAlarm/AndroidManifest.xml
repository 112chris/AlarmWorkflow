<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.florianritterhoff.eAlarm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <permission
        android:name="de.florianritterhoff.eAlarm.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="de.florianritterhoff.eAlarm.permission.C2D_MESSAGE" />

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="16" />

    <application
        android:name="de.florianritterhoff.eAlarm.AlarmClient"
        android:allowBackup="false"
        android:allowClearUserData="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="false"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.florianritterhoff.eAlarm.OperationView"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.florianritterhoff.eAlarm.Settings" />
        <activity android:name="de.florianritterhoff.eAlarm.About" />
        <activity android:name="de.florianritterhoff.eAlarm.C2DMClientActivity" />
        <activity android:name="de.florianritterhoff.eAlarm.OperationDetail" />
        <activity android:name="de.florianritterhoff.eAlarm.Map" />

        <receiver
            android:name=".services.GCMReciver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="de.florianritterhoff.eAlarm" />
            </intent-filter>
        </receiver>

        <service android:name=".services.GCMIntent" />

        <uses-library android:name="com.google.android.maps" />
    </application>

</manifest>