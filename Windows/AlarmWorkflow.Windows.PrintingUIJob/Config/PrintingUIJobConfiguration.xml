<?xml version="1.0" encoding="utf-8" ?>
<PrintingConfiguration>
  <!-- The Uri to the print server to use (i. e. "\\printsrv"). Leave blank to use the local print server. -->
  <PrintServer></PrintServer>
  <!-- If printer name is empty, the printer defined as default is used -->
  <PrinterName></PrinterName>
  <CopyCount>1</CopyCount>
  <!-- It has been reported that sometimes, when printing, some parts or all of the view parts are empty. 
       This wait-interval has the purpose of giving the UI some time before printing (in milliseconds).
       Don't set this interval too high, this may block the UI.
  -->
  <WaitInterval>50</WaitInterval>
  <!-- Set this option to "true" to avoid printing operations that have already been printed -->
  <RememberPrintedOperations>True</RememberPrintedOperations>
</PrintingConfiguration>