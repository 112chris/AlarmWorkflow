<ControlInformation Name="ILS Ansbach Fax">
  
  <Section Text="Alarmfax">
    <Area Text="Einsatznummer" MapTo="OperationNumber" />
  </Section>

  <Section Text="Mitteiler">
    <Area Text="Name" MapTo="Messenger" />
  </Section>

  <Section Text="Einsatzort">
    <!-- Hinweis: Faxe der ILS Ansbach haben eine zweite Spalte "Haus-Nr.". Hier nicht berücksichtigt! -->
    <Area Text="Straße" MapTo="Einsatzort.Street" />
    <!-- Hinweis: Die PLZ ist enthalten.  -->
    <Area Text="Ort" MapTo="Einsatzort.City" />
    <Area Text="Objekt" MapTo="Einsatzort.Property" />
    <Area Text="Station" MapTo="Einsatzort Station" />
  </Section>

  <Section Text="Zielort">
    <Area Text="Straße" MapTo="Zielort.Street" />
    <Area Text="Ort" MapTo="Zielort.City" />
    <Area Text="Objekt" MapTo="Zielort.Property" />
    <Area Text="Station" MapTo="Zielort Station" />
  </Section>

  <Section Text="Einsatzgrund">
    <Area Text="Schlagw" MapTo="Keywords.Keyword" />
    <Area Text="Stichwort B" MapTo="Keywords.B" />
    <Area Text="Stichwort R" MapTo="Keywords.R" />
    <Area Text="Stichwort S" MapTo="Keywords.S" />
    <Area Text="Stichwort T" MapTo="Keywords.T" />
    <Area Text="Prio" MapTo="Priority" />
  </Section>

  <Section Text="Einsatzmittel">
    <!-- "Einsatzmittel" ist ein Abschnitt dynamischer Größe, derzeit nicht auswertbar im
          Universalparser. Kommt mit einem zukünftigen Update! -->
  </Section>

  <Section Text="Bemerkung">
    <!-- "Bemerkung" ist ein Abschnitt dynamischer Größe, derzeit nicht auswertbar im
          Universalparser. Kommt mit einem zukünftigen Update! -->
  </Section>

  <Section Text="Ende fax">
    <!-- Dieser Abschnitt wird ignoriert. -->
  </Section>
  
</ControlInformation>