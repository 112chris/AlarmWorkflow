<?xml version="1.0" encoding="utf-8" ?>
<AlarmWorkflow>
  <FaxPath>C:\Fax</FaxPath>
  <ArchivePath>C:\Fax\Archive\</ArchivePath>
  <AnalysisPath>C:\Fax\Analysis\</AnalysisPath>
  <!-- if path is empty. Alarmworkflow will use the standard path. -->
  <OCRSoftware type="cuneiform" path="" />
  <AlarmfaxParser>MucLandParser</AlarmfaxParser>
  <OperationStore>SQLCEDatabaseOperationStore</OperationStore>
  
  <!-- This section states which exports are enabled and can be used.
       You should manually add and enable only those you trust and need.
  -->
  <ExportWhitelist>
    <Export Name="DisplayWakeUpJob" IsEnabled="False" />
    <Export Name="MailingJob" IsEnabled="False" />
    <Export Name="MySqlDatabaseJob" IsEnabled="False" />
    <Export Name="SmsJob" IsEnabled="False" />
    <Export Name="DisplayWakeUpJob" IsEnabled="False" />

    <Export Name="IlsAnsbachParser" IsEnabled="True" />
    <Export Name="MucLandParser" IsEnabled="True" />

    <Export Name="SQLCEDatabaseJob" IsEnabled="True" />
    <Export Name="SQLCEDatabaseOperationStore" IsEnabled="True" />

    <Export Name="IlsAnsbachOperationViewer" IsEnabled="True" />
  </ExportWhitelist>

</AlarmWorkflow>